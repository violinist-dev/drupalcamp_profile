{
    "name": "drupalnorge/drupalcamp_profile",
    "description": "The profile for drupalcamps.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "patches": {}
    },
    "require": {
        "drupal/core": "^8.6.14",
        "drupal/redirect": "1.3.0",
        "drupal/admin_toolbar": "1.27.0",
        "drupal/coffee": "1.1.0",
        "drupal/reroute_email": "1.0.0",
        "drupal/sparkpost": "2.0.0",
        "drupal/email_registration": "1.0.0-rc6",
        "drupalnorge/drupalcamp_theme": "*",
        "drupal/google_analytics": "2.3.0"
    }
}
